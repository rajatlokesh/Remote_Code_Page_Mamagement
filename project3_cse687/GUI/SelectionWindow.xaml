<Window x:Class="GUI.SelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="SelectionWindow" Height="300" Width="500" Loaded="Window_Loaded" Unloaded="Window_Unloaded">
  <!-- 
    /////////////////////////////////////////////////////////////////////
    // SelectionWindow.xaml.cs - Prototype for OOD Project #4          //
    //                                                                 //
    // Jim Fawcett, CSE687 - Object Oriented Design, Fall 2018         //
    /////////////////////////////////////////////////////////////////////
  -->
  <Grid Margin="5">
        <ListBox x:Name="Selections" FontFamily="tahoma" SelectionMode="Extended" SelectionChanged="Selections_SelectionChanged"></ListBox>
  </Grid>
</Window>
